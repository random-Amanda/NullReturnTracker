<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.plugin.trackNullReturn.commands.category"
            name="Sample Category">
      </category>
      <command
            categoryId="org.plugin.trackNullReturn.commands.category"
            name="Sample Command"
            id="org.plugin.trackNullReturn.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.plugin.tracknullreturn.handlers.SampleHandler"
            commandId="org.plugin.trackNullReturn.commands.sampleCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.plugin.trackNullReturn.commands.sampleCommand"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="org.plugin.trackNullReturn.menus.sampleMenu"
               label="Sample Menu"
               mnemonic="M">
            <command
                  commandId="org.plugin.trackNullReturn.commands.sampleCommand"
                  id="org.plugin.trackNullReturn.menus.sampleCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.plugin.trackNullReturn.toolbars.sampleToolbar">
            <command
                  id="org.plugin.trackNullReturn.toolbars.sampleCommand"
                  commandId="org.plugin.trackNullReturn.commands.sampleCommand"
                  icon="icons/sample.png"
                  tooltip="Refactoring Assitant">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
       id="nullReturnMarker"
       name="Null Return Marker"
       point="org.eclipse.core.resources.markers">
    <super
          type="org.eclipse.jdt.core.problem">
    </super>
    <super
          type="org.eclipse.core.resources.problemmarker">
    </super>
    <super
          type="org.eclipse.core.resources.textmarker">
    </super>
    <persistent
          value="true">
    </persistent>
 </extension>
</plugin>
